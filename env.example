# SMTP_HOST=
# SMTP_PORT=
# SMTP_USER=
# SMTP_PASS=

# Payment Settings (optional - for future implementation)
# STRIPE_SECRET_KEY=
# STRIPE_PUBLISHABLE_KEY=
